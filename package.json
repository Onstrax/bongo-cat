    "keywords": [
        "obsidian",
        "obsidian-plugin",
        "bongo-cat",
        "animation"
    ],
    "author": "Onstrax",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "esbuild": "0.25.5",
        "eslint-plugin-obsidianmd": "0.1.9",
        "globals": "14.0.0",
        "tslib": "2.4.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "8.35.1",
        "@eslint/js": "9.30.1",
        "jiti": "2.6.1"
    },
    "dependencies": {
        "obsidian": "latest"
    }
